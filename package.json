{
  "name": "wesl-site",
  "type": "module",
  "dependencies": {
    "@weborigami/async-tree": "0.3.2",
    "@weborigami/origami": "0.3.2",
    "@weborigami/pagefind": "0.0.5"
  },
  "scripts": {
    "build": "ori copy src/site.jse, clear files:build",
    "dev": "run-s start",
    "deploy:site": "run-s build pages:deploy",
    "pages:deploy": "wrangler pages deploy --project-name wesl-lang build",
    "start": "ori serve watch ., =debug src/site.jse"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "wrangler": "^4.16.1"
  }
}
